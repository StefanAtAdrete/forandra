        "drupal/pathauto": "^1.11",
        "drupal/physical": "^1.3",
        "drupal/publication_date": "^2.0@beta",
        "drupal/quickedit": "^1.0",
        "drupal/rdf": "^2.1",
        "drupal/redirect": "^1.8",
        "drupal/remote_stream_wrapper": "^2.0",
        "drupal/responsive_menu": "^4.4",
        "drupal/responsive_tables_filter": "^1.17",
