        "drupal/language_switcher_extended": "^1.1",
        "drupal/language_switcher_langcode": "^2.1",
        "drupal/languageicons": "^2.0@beta",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_iframe_modal": "^1.3",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/libraries": "^4.0",
        "drupal/linkit": "^6.0@beta",
        "drupal/mailsystem": "^4.4",
        "drupal/media_library_form_element": "^2.0",
        "drupal/media_remote": "^1.7",
        "drupal/metatag": "^1.22",
        "drupal/oembed_providers": "^2.1",
